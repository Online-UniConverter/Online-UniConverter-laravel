{
    "name": "onlineuniconvert/onlineuniconvert-laravel",
    "description": "Laravel PHP SDK for OnlineUniConvert APIs",
    "type": "library",
    "license": "MIT",
    "keywords": ["convert", "laravel"],
    "homepage": "https://github.com/Online-UniConverter/Online-UniConverter-laravel",
    "authors": [
        {
            "name": "00000000",
            "email": "xxx@xx.cn"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "illuminate/container": "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/http": "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0|^8.0",
        "symfony/psr-http-message-bridge": "^1.2|^2.0",
        "nyholm/psr7": "^1.2",
        "onlineuniconvert/onlineuniconvert-php": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "OnlineUniConvert\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnlineUniConvert\\Laravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OnlineUniConvert\\Laravel\\Providers\\OnlineUniConvertServiceProvider"
            ],
            "aliases": {
                "OnlineUniConvert": "OnlineUniConvert\\Laravel\\Facades\\OnlineUniConvert"
            }
        }
    },
    "repositories": {
        "sdk": {
            "type": "path",
            "url": "D:\\project\\php\\mediaiosdk\\php"
        }
    }
}
